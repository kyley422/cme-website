{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup:docker": "docker build -f .dev/vercel-blob-server.containerfile -t vercel-blob-server .dev",
    "setup:podman": "podman build -f vercel-blob-server.containerfile -t vercel-blob-server .dev",
    "dev:podman": "dotenv -e '.env.local' -- podman-compose",
    "dev:docker": "dotenv -e '.env.local' -- docker-compose",
    "dev:seed": "dotenv -e '.env.local' -- tsx 'seed'",
    "dev:pgcli": "dotenv -e '.env.local' -- sh -c 'pgcli \"$POSTGRES_URL\"'",
    "dev:drizzle": "dotenv -e '.env.local' -- drizzle-kit",
    "prod:pgcli": "dotenv -e '.env.production' -- sh -c 'pgcli \"$POSTGRES_URL\"'",
    "prod:drizzle": "dotenv -e '.env.production' -- drizzle-kit",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix --ext .js,.jsx ."
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/next-js": "^2.4.2",
    "@chakra-ui/react": "^2.10.9",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource-variable/outfit": "^5.2.5",
    "@fontsource/outfit": "^5.2.5",
    "@neondatabase/serverless": "^1.0.0",
    "@tabler/icons-react": "^3.33.0",
    "@tailwindcss/postcss": "^4.1.7",
    "@vercel/blob": "^1.0.2",
    "argon2": "^0.41.1",
    "classnames": "^2.5.1",
    "drizzle-orm": "^0.43.1",
    "eslint": "8.44.0",
    "eslint-config-next": "15.3.1",
    "framer-motion": "^10.18.0",
    "jose": "^6.0.11",
    "next": "^15.3.2",
    "postcss": "^8.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^4.12.0",
    "react-parallax": "^3.5.2",
    "react-responsive": "^10.0.1",
    "react-slick": "^0.29.0",
    "server-only": "^0.0.1",
    "slick-carousel": "^1.8.1",
    "swiper": "^11.2.7",
    "tailwindcss": "^4.1.7",
    "use-sound": "^4.0.4",
    "zod": "^3.25.17",
    "zod-form-data": "^2.0.7"
  },
  "devDependencies": {
    "@types/pg": "^8.15.2",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.31.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-sort-destructure-keys": "^1.6.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "mini-css-extract-plugin": "^2.9.2",
    "neonctl": "^2.9.2",
    "pg": "^8.16.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
